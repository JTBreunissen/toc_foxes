<div class="jumbotron">
  <h1>In-Company Requests</h1>
</div>
<div class="container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1" class="center-block">
      <div class="container">
        <div class="row">
          <div class="col-md-3">
            <h4><strong>Workshop</strong></h4>
          </div>
          <div class="col-md-3">
            <h4><strong>Company</strong></h4>
          </div>
          <div class="col-md-3">
            <h4><strong>Email</strong></h4>
          </div>
          <% @in_company_requests.each do |in_company_request| %>
          <div class="col-md-3">
            <h4>in_company_request.workshop.title</h4>
          </div>
          <div class="col-md-3">
            <h4>in_company_request.company_name</h4>
          </div>
          <div class="col-md-3">
            <h4>in_company_request.email</h4>
          </div>
          <div class="col-md-3">
            <h4>
              <%= link_to "View", admin_in_company_request_path(in_company_request), class: "btn btn-default" %>
              <%= link_to "Edit", edit_admin__in_company_request_path(in_company_request), class: "btn btn-default" %>
              <%= link_to "Delete", admin__in_company_request_path(in_company_request), class: "btn btn-default", :confirm => "Are you sure?", :method => :delete %>
            </h4>
          </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
