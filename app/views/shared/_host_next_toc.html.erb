<section>
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-6 col-md-offset-3" class="center-block">
        <h1 class="text-center">Host next TOC</h1>
        <h3 class="text-center">Talk to us</h3>
        <form>
          <div class="form-group">
            <input type="first_name" class="form-control" placeholder="First Name">
          </div>
          <div class="form-group">
            <input type="last_name" class="form-control" placeholder="Last Name">
          </div>
          <div class="form-group">
            <input type="company_name" class="form-control" placeholder="Company Name">
          </div>
          <div class="form-group">
            <input type="email" class="form-control" placeholder="Email">
          </div>
          <div class="form-group">
            <input type="date" id="myDate" class="form-control" placeholder="Date">
          </div>
          <div class="form-group">
            <input type="number" class="form-control" placeholder="Number of participants">
          </div>
          <div class="form-group">
            <textarea class="form-control" rows="10" cols="75" placeholder="Comments"></textarea>
          </div>
          <div class="text-center">
            <button type="submit" class="btn btn-default">Submit</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</section>
